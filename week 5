#Resolving Variable Name Conflicts
class Student{
    String name="Alice";
    Student(String name){
        this.name=name; //refers to instance variable
    }

     void display(){
        System.out.println("Name: "+this.name);
    }
}

public class Prac1 {
    public static void main(String[] args) {
        Student s1=new Student("Alex");
        s1.display();
    }
}

#Calling Constructor
class Demo{
    Demo() {
        this("Default Constructor"); //calls parameterized constructor
        System.out.println("No-argument Constructor");
    }

    Demo(String msg){
        System.out.println("Parameterized Constructor: " + msg);
    }
}

public class Prac1 {
    public static void main(String[] args) {
        new Demo();
    }
}
